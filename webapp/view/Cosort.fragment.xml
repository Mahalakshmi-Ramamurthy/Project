<core:FragmentDefinition
	id="tree"
	xmlns="sap.m"
	xmlns:ui="sap.ui.table"
	xmlns:core="sap.ui.core"
	xmlns:custom="com.order.order.view">
	 
 		<List
		id="ShortProductList" 
		items="{
			path: '/custSomeData',
			sorter: {
				path: 'HandlingUnit'
			}
		}">
		<items>
		<ObjectListItem  type="Active"
		 				press="handleSOSelectco"
		 				title="{HandlingUnit}">
						<attributes> 
						<ObjectAttribute text="{HandlingUnit}" visible="false"></ObjectAttribute>
		 				</attributes>

					</ObjectListItem>
		</items>
	</List>
 	 
 	 
	 
</core:FragmentDefinition>