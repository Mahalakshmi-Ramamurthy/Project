<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" >
     
   <f:Form id="myorders"
			editable="true" class="sapUiContentPadding sapUiSizeCompact"  >
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="12"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="0"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="2"
					columnsM="2"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					
					<f:formElements>
						<f:FormElement label="Order Number" >
							<f:fields>
				            <Input id="CofInq" placeholder="Enter" type="Number">  </Input>
							</f:fields>
					</f:FormElement>
					
					<f:FormElement label="Date Placed" >
							<f:fields>
				            <DatePicker placeholder="Select" id="CofInqDate" value="" valueFormat="yyyy-MM-dd" displayFormat="long" class="sapUiSmallMarginBottom"/> 
							</f:fields>
					</f:FormElement>
						
				
						
					 
					
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	 
     
</core:FragmentDefinition>