<core:FragmentDefinition
	id="vtree2"
	xmlns="sap.m"
	xmlns:ui="sap.ui.table"
	xmlns:core="sap.ui.core"
	xmlns:custom="ship.ship.views">
	 
 		<List
		id="listtree" 
		items="{
			path: '/sortodata',
			sorter: {
				path: 'HandlingUnit'
			}
		}">
		<items>
		<ObjectListItem  type="Active"
		 				press="handlesort2"
		 				title="{HandlingUnit}">
						<attributes> 
						<ObjectAttribute text="{HandlingUnit}" visible="false"></ObjectAttribute>
		 				</attributes>

					</ObjectListItem>
		</items>
	</List>
 	 
 	 
	 
</core:FragmentDefinition>